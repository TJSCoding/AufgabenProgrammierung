public class ArrayAusgabe {
	
	public static void main(String[] args) {
		
		String[] stringSammlung = {"Sehr","gut","gemacht","!"};
		anzeigenStringArray (stringSammlung);
		
	}
	
	public static void anzeigenStringArray (String[] stringSammlung) {
		
		for (String string: stringSammlung) {
			
			System.out.println(string + " ");
		
		}
		
	}

}
