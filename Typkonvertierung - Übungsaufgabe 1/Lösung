public class TypkonvertierungAufgabe1 {
	
	public static void main (String []args) {
		
		int zahl1 = 71;
		int zahl2 = 85;
		int zahl3 = 84;
		int zahl4 = 33;
		
		char b1 = (char) zahl1;
		char b2 = (char) zahl2;
		char b3 = (char) zahl3;
		char b4 = (char) zahl4;
		
		String ausgabe = b1+""+b2+""+b3+""+b4;
		
		System.out.println(ausgabe);	
		
	}

}
